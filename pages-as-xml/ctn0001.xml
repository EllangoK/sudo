          <InclusionCriterion>Seeking treatment</InclusionCriterion>
          <InclusionCriterion>Outpatient or Inpatient</InclusionCriterion>
        </InclusionCriterionSet>
      </Enrollment>
    </PopulationSpecification>
    <PopulationSpecification>
      <Enrollment>
        <ExclusionCriterionSet>None</ExclusionCriterionSet>
    </Enrollment>
  </PopulationSpecification>
